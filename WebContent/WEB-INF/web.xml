<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://java.sun.com/xml/ns/javaee" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>YelpClient</display-name>
  <welcome-file-list>
    <welcome-file>/View/signup.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>signup</display-name>
    <servlet-name>signup</servlet-name>
    <servlet-class>Servlets.Signup</servlet-class>
  </servlet>
  <servlet>
    <display-name>homepage</display-name>
    <servlet-name>homepage</servlet-name>
    <servlet-class>Servlets.Homepage</servlet-class>
  </servlet>
  <servlet>
    <display-name>signin</display-name>
    <servlet-name>signin</servlet-name>
    <servlet-class>Servlets.Signin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/View/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>homepage</servlet-name>
    <url-pattern>/View/homepage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>signin</servlet-name>
    <url-pattern>/View/signin</url-pattern>
  </servlet-mapping>
</web-app>